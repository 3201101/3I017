<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://java.sun.com/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
	version="3.0">

	<display-name>Ln - Log À Rythmes</display-name>
	<description>Ln - Log À Rythmes - Projet d'application web du binôme Loglisci et Nasturas.</description>

	<servlet>
		<servlet-name>API</servlet-name>
		<servlet-class>ln.app.API</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>API</servlet-name>
		<url-pattern>/api</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>API</servlet-name>
		<url-pattern>/api/*</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>Index</servlet-name>
		<servlet-class>ln.app.Index</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Index</servlet-name>
		<url-pattern></url-pattern>
	</servlet-mapping>
	
</web-app>